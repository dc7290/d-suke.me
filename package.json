{
  "name": "d-suke.me",
  "description": "This repository is the source for https://d-suke.me",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "create:fc": "hygen new fc",
    "create:pc": "hygen new pc",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch --output src/utils",
    "build": "pathpida --output src/utils && next build",
    "start": "next start",
    "fix": "prettier --write './**/*.{js,jsx,ts,tsx}'",
    "lint": "next lint . --cache",
    "lint:fix": "next lint . --cache --fix",
    "lint-staged": "lint-staged",
    "tsc": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.5.0",
    "@heroicons/react": "1.0.6",
    "clsx": "1.1.1",
    "dayjs": "1.10.8",
    "focus-visible": "5.2.0",
    "lazysizes": "5.3.2",
    "microcms-js-sdk": "2.0.0",
    "next": "12.1.0",
    "next-head-seo": "0.1.3",
    "react": "17.0.2",
    "react-aria": "3.14.0",
    "react-dom": "17.0.2",
    "react-spinners": "0.11.0",
    "rss-parser": "3.12.0",
    "scroll-behavior-polyfill": "2.0.13",
    "tailwindcss": "3.0.23"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.1.0",
    "@types/node": "16.11.26",
    "@types/react": "17.0.40",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "autoprefixer": "10.4.4",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "hygen": "6.2.0",
    "lint-staged": "12.3.7",
    "npm-run-all": "4.1.5",
    "pathpida": "0.18.0",
    "postcss": "8.4.12",
    "postcss-import": "14.0.2",
    "prettier": "2.6.0",
    "prettier-plugin-tailwindcss": "0.1.8",
    "typescript": "4.6.2"
  },
  "volta": {
    "node": "16.14.2",
    "yarn": "1.22.18"
  }
}
